CREATE TABLE `portx`.`container_log_history` ( `id` INT(11) NULL DEFAULT NULL AUTO_INCREMENT , `container_id` INT(11) NULL DEFAULT NULL , `activity_id` INT(11) NULL DEFAULT NULL , `note` TEXT NULL DEFAULT NULL , `user_id` INT(11) NULL DEFAULT NULL , `status` ENUM('CREATED','UPDATED','DELETED') NOT NULL , `date` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`)) ENGINE = InnoDB; 
ALTER TABLE `container_log_history` ADD INDEX(`container_id`), ADD INDEX(`activity_id`), ADD INDEX(`user_id`);  
CREATE TABLE `portx`.`proforma_container_log_history` ( `id` INT(11) NULL DEFAULT NULL AUTO_INCREMENT , `container_id` INT(11) NULL DEFAULT NULL , `activity_id` INT(11) NULL DEFAULT NULL , `note` TEXT NULL DEFAULT NULL , `user_id` INT(11) NULL DEFAULT NULL , `status` ENUM('CREATED','UPDATED','DELETED') NOT NULL , `date` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`)) ENGINE = InnoDB; 
ALTER TABLE `proforma_container_log_history` ADD INDEX(`container_id`), ADD INDEX(`activity_id`), ADD INDEX(`user_id`);  

CREATE TABLE `portx`.`container_ucl_history` ( `id` INT(11) NULL DEFAULT NULL AUTO_INCREMENT , `container_id` INT(11) NULL DEFAULT NULL , `type` ENUM('GATE IN','GATE OUT') NOT NULL , `depot_id` INT(11) NULL DEFAULT NULL , `gate_id` INT(11) NULL DEFAULT NULL , `vehicle_id` INT(11) NULL DEFAULT NULL , `driver_id` INT(11) NULL DEFAULT NULL , `trucking_company_id` INT(11) NULL DEFAULT NULL , `status` ENUM('CREATED','UPDATED','DELETED') NOT NULL , `date` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`));
ALTER TABLE `container_ucl_history` ADD INDEX(`container_id`), ADD INDEX(`depot_id`), ADD INDEX(`gate_id`), ADD INDEX(`vehicle_id`), ADD INDEX(`driver_id`), ADD INDEX(`trucking_company_id`);  
CREATE TABLE `portx`.`proforma_container_ucl_history` ( `id` INT(11) NULL DEFAULT NULL AUTO_INCREMENT , `container_id` INT(11) NULL DEFAULT NULL , `type` ENUM('GATE IN','GATE OUT') NOT NULL , `depot_id` INT(11) NULL DEFAULT NULL , `gate_id` INT(11) NULL DEFAULT NULL , `vehicle_id` INT(11) NULL DEFAULT NULL , `driver_id` INT(11) NULL DEFAULT NULL , `trucking_company_id` INT(11) NULL DEFAULT NULL , `status` ENUM('CREATED','UPDATED','DELETED') NOT NULL , `date` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`));
ALTER TABLE `proforma_container_ucl_history` ADD INDEX(`container_id`), ADD INDEX(`depot_id`), ADD INDEX(`gate_id`), ADD INDEX(`vehicle_id`), ADD INDEX(`driver_id`), ADD INDEX(`trucking_company_id`);  

CREATE TABLE `portx`.`container_ucl_history` ( `id` INT(11) NOT NULL AUTO_INCREMENT , `container_id` INT(11) NULL DEFAULT NULL , `status` ENUM('CREATED','UPDATED','DELETED') NOT NULL , `date` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`)); 
ALTER TABLE `container_ucl_history` ADD INDEX(`container_id`); 